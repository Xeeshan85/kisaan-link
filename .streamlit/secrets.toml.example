# Streamlit Cloud Secrets Template
# Copy these to your Streamlit Cloud app's Secrets section
# Settings → Secrets → Paste and save

# Required: Google AI API Key for Gemini
GOOGLE_API_KEY = "your_google_api_key_here"

# Required: Google Cloud Project ID for Earth Engine
EE_PROJECT = "your_gcp_project_id_here"

# Optional: Override default settings
# LLM_MODEL = "gemini-2.5-flash"
# LLM_TEMPERATURE = "0"
# DEFAULT_LAT = "31.5204"
# DEFAULT_LON = "74.3587"
